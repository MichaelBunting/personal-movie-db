{
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "jsx-a11y/label-has-for": [2, {
      "required": {
          "every": ["id"]
      },
      "allowChildren": false
    }]
  },
  "globals": {
    "document": false
  },
}